<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Posts</title>
    <link rel="stylesheet" href="/abc/css/adminlist.css">
</head>
<body>
    
    
        <fieldset>
            <legend>All Posts</legend>
           
            <table border="1">
                <tr>
                    <td>Id</td>
                    <td>Title</td>
                    <td>Body</td>
                    <td>Poster</td>
                    
                    <td>Action</td>
                </tr>
                
                <% posts.forEach((post) => { %>
                    <tr>
                        <td><%= post.id %></td>
                        <td><%= post.title %></td>
                        <td><%= post.post_body %></td>
                        <td><%= post.buyer_name %></td>
                        
                        <td>
                            <a href="/seller/message/<%= post.id %>"><button style="color: green;">message</button></a>
                            <a href=""><button style="color: violet;">Upload Work</button></a>
                        </td>
                    </tr>
                <% }); %>
    
            </table>
            <br>
            <a href="/seller/sellerController"><button>Back</button></a>
        </fieldset>
    
    
    <style>
        td{
            text-align: center;
            padding: 2px 14px;
        }
    </style>
    
</body>
</html>